int main () {
    int i = 1;
    while (i < 5) {
        int x = 8 - i;
        printInt(x);
        int y = i * (-2);
        printInt(y);
        i++;
    }

    int j = 3;
    int k = 3;
    while (j > 0) {
        j = j - 2;
        printInt(j * 17);
        printInt(k * 2);
        k = 2 - k;
        printInt(k * 2);
    }

    return 0;
}
